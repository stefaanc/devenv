---

apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  name: terminal
  namespace: terminal
  labels:
    app: terminal
  annotations:
    kubernetes.io/ingress.class: "nginx"
    nginx.ingress.kubernetes.io/proxy-read-timeout: "600"
spec:
  rules:
  - 
    http:
      paths:
      - 
        path: /
        backend:
          serviceName: terminal
          servicePort: registry
